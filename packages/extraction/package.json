{
  "name": "@doctype/fluent-react-extract",
  "version": "0.1.3",
  "description": "String extraction CLI for @fluent/react",
  "keywords": [
    "udacity",
    "fluent",
    "localization",
    "l10n",
    "ftl",
    "translate",
    "translation",
    "fluent-react",
    "internationalization",
    "i18n",
    "components",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/doctype/fluent-react-utils"
  },
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "bin": {
    "l10n": "lib/cli.js"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "test": "yarn lint && yarn cover",
    "test:prod": "yarn test",
    "test:only": "mocha --require @babel/register --recursive",
    "cover": "nyc _mocha -- --require @babel/register --recursive",
    "lint": "eslint src test",
    "build": "babel src --out-dir lib",
    "prepublish": "yarn clean && yarn test && yarn build"
  },
  "dependencies": {
    "@babel/parser": "^7.1.0",
    "@fluent/syntax": "^0.17.0",
    "babylon-walk": "^1.0.2",
    "glob": "^7.1.3",
    "jsx-ast-utils": "^3.2.0",
    "mkdirp": "^1.0.4",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@doctype/fluent-react-components": "^0.1.3",
    "@fluent/react": "^0.13.1",
    "chai": "^4.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  },
  "optionalDependencies": {
    "@doctype/fluent-react-components": "^0.1.3"
  }
}
