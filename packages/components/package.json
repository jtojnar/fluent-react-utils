{
  "name": "@doctype/fluent-react-components",
  "version": "0.1.3",
  "description": "Utility components for fluent-react",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "yarn lint && yarn cover",
    "test:prod": "yarn test",
    "test:only": "mocha --require @babel/register --recursive",
    "cover": "nyc _mocha -- --require @babel/register --recursive",
    "lint": "eslint src test",
    "build": "babel src --out-dir lib",
    "prepublish": "yarn clean && yarn test && yarn build"
  },
  "keywords": [
    "udacity",
    "fluent",
    "localization",
    "l10n",
    "ftl",
    "translate",
    "translation",
    "fluent-react",
    "internationalization",
    "i18n",
    "components",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/doctype/fluent-react-utils"
  },
  "files": [
    "lib",
    "src"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@fluent/react": "^0.13.1",
    "chai": "^4.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2"
  }
}
